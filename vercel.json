{
    "builds":[
        {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
            "includesFiles": ["db.json"]
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
          }
    ]
}